<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox styleClass="root" alignment="CENTER" spacing="14.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projects.SecondaryController">
    <padding>
        <Insets bottom="24.0" left="24.0" right="24.0" top="24.0" />
    </padding>

    <children>
        <Label text="Quiz Complete" styleClass="title" />
        <Label fx:id="scoreLabel" text="Score: 0/0 (0%)" styleClass="question" />
        <Label fx:id="messageLabel" text="" styleClass="muted" />
        <Button text="Restart" onAction="#restartQuiz" styleClass="primary-button" />
    </children>
</VBox>
